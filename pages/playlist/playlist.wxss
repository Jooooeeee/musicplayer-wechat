.container{
  margin: 0;
  padding: 0;
  overflow: hidden;
}
.divLine{
 background: #313131;
 width: 100%;
 height: 2rpx;
}
.songcontainer{
  display: flex;
  flex-direction: row;
  width: 100%
}
.song{
  width: 80%;
  border-bottom:#313131;
}
.songs{
  width: 100%;
    display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column
}
.songName{
  font-size: 30rpx;
  display: block;
  color:#313131;
}
.artists {
  font-size: 25rpx;
  color:#313131;
}
.album{
  font-size: 25rpx;
  color:#313131;
}
.songpic{
  height: 45px;
  width: 45px;
  margin-left: 10px;
  margin-right: 10px;
  margin-top: 2px;
  margin-bottom: 2px
}
.playlistName{
  color: #313131;
  display: block;
  margin-bottom: 10px;
}